{
    "name": "base85",
    "version": "2.2.0",
    "description": "Base85 (Ascii85) encode and decode functionality",
    "main": "dist/base85.cjs.js",
    "module": "dist/base85.esm.js",
    "scripts": {
        "build": "esbuild ./src/base85.js --format=cjs --outfile=./dist/base85.cjs.js && esbuild ./src/base85.js --format=esm --outfile=./dist/base85.esm.js",
        "test": "jshint ./src && nodeunit tests"
    },
    "devDependencies": {
        "esbuild": "^0.9.2",
        "jshint": "^2.9.6",
        "nodeunit": "^0.11.3",
        "underscore": "^1.11.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/noseglid/base85"
    },
    "keywords": [
        "base85",
        "ascii85",
        "encode",
        "decode",
        "binary-to-text"
    ],
    "author": "Alexander Olsson <noseglid@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/noseglid/base85/issues"
    },
    "homepage": "https://github.com/noseglid/base85"
}
